{
   "name": "backbone-shopping-cart",
   "version": "1.0.0",
   "description": "Shooping Cart Application using Backbone.js and css Flex design",
   "main": "index.js",
   "repository": {
      "type": "git",
      "url": "https://gitlab.mynisum.com/dmantena/backbone-shopping--cart.git"
   },
   "author": "Dheeraj Mantena",
   "license": "ISC",
   "scripts": {
      "start": "concurrently \"npm run server\" \"npm run dev:backbone\"",
      "server": "node server/server.js",
      "webpack-dev-server": "webpack-dev-server",
      "webpack": "webpack ",
      "prod": "npm run webpack -- --env.mode production",
      "dev": "npm run webpack-dev-server -- --env.mode development --hot --history-api-fallback",
      "dev:backbone": "npm run dev -- --env.presets backbone",
      "prod:backbone": "npm run prod -- --env.presets backbone",
      "test": "jasmine test/*.spec.js"
   },
   "devDependencies": {
      "@babel/core": "^7.0.0-beta.46",
      "@babel/plugin-syntax-dynamic-import": "^7.2.0",
      "@babel/preset-env": "^7.0.0-beta.46",
      "autoprefixer": "^9.3.1",
      "babel-loader": "^8.0.0-beta",
      "chai": "^4.1.2",
      "compression-webpack-plugin": "^1.1.11",
      "concurrently": "^4.1.0",
      "css-loader": "^0.28.11",
      "extract-loader": "^1.0.2",
      "file-loader": "^1.1.11",
      "fs": "0.0.1-security",
      "handlebars-helpers": "^0.10.0",
      "html-webpack-plugin": "^3.2.0",
      "jasmine": "^3.3.1",
      "json-server": "^0.14.2",
      "mini-css-extract-plugin": "^0.4.0",
      "mocha": "^5.2.0",
      "node-sass": "^4.9.3",
      "plugin": "^0.3.3",
      "postcss-loader": "^3.0.0",
      "provide": "^0.1.3",
      "raw-loader": "^1.0.0",
      "sass-loader": "^6.0.4",
      "style-loader": "^0.21.0",
      "url-loader": "^1.0.1",
      "webpack": "^4.25.1",
      "webpack-cli": "^3.1.2",
      "webpack-dev-server": "^3.1.10",
      "webpack-merge": "^4.1.2",
      "webpack-node-externals": "^1.7.2"
   },
   "dependencies": {
      "@types/jquery": "^3.3.29",
      "backbone": "^1.3.3",
      "handlebars": "^4.1.0",
      "handlebars-loader": "^1.7.1",
      "jquery": "^3.3.1",
      "underscore": "^1.9.1"
   }
}
